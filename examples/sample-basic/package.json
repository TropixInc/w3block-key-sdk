{
  "name": "@tropixinc/w3block-key-sdk-sdk",
  "version": "0.0.0",
  "description": "W3block Key SDK",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node tools/cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node tools/cleanup src/types && tsc -p config/tsconfig.types.json && npm run copy-dts",
    "clean": "node tools/cleanup",
    "package": "npm run build && npm pack",
    "test": "jest --no-cache --runInBand",
    "test:cov": "jest --coverage --no-cache --runInBand",
    "addscope": "node tools/packagejson name @tropixinc/pixwayid-sdk",
    "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" dist/types",
    "lint": "eslint . --fix",
    "release": "standard-version",
    "git:add": "git add --all",
    "git:commit": "git-cz",
    "git": "npm run git:add && npm run git:commit",
    "check-types": "tsc --incremental false --noEmit"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "pixchain",
    "sdk"
  ],
  "author": "Gislainy Velasco <gislainy@tropix.io>",
  "license": "MIT",
  "homepage": "https://github.com/TropixInc/w3block-key-sdk",
  "repository": {
    "type": "git",
    "url": "git@github.com:TropixInc/w3block-key-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/TropixInc/w3block-key-sdk/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "husky": "^7.0.4",
    "jest": "^27.2.0",
    "lint-staged": "^12.1.4",
    "pinst": "^2.1.6",
    "prettier": "^2.4.0",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.5.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@metamask/eth-sig-util": "4.0.0",
    "axios": "^0.24.0",
    "ethers": "^5.5.2",
    "lodash": "^4.17.21",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "query-string": "^7.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  }
}